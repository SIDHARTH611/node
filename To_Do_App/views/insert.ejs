<div class="row mt-5">
  <div class="col-md-6 m-auto">
    <div class="card card-body">
      <h1 class="text-center mb-3"><i class="fas fa-sign-in-alt"></i>  INSERT</h1>
      <%-include('./partials/messages.ejs');%>



      <form action="/insert" method="post">
        <div class="form-group">
          <label for="code">ID</label>
            <select
            type="text"
            id="name"
            name="code"
            class="form-control"
            placeholder="Enter code"
            value="<%= typeof code != 'undefined' ? code : '' %>"
          >
          <option value="<%= user.email %> 1"> personal</option>
          <option value="<%= user.email %> 2"> professional</option>
          </select>
        <div class="form-group">
          <label for="da1">DATE</label>
          <input
            type="date"
            id="da1"
            name="da1"
            class="form-control"
            placeholder="Enter da1"
            value="<%= typeof da1 != 'undefined' ? da1 : '' %>"
          />
        </div>
        <div class="form-group">
          <label for="task">TASK</label>
          <input
            type="text"
            id="task"
            name="task"
            class="form-control"
            placeholder="Enter TASK"
            value="<%= typeof task != 'undefined' ? task : '' %>"
          
          />
        </div>
        <button type="submit" class="btn btn-primary btn-block">INSERT</button>
        
        </form>
        <form action ="/dashboard/submit" method="post">
          <button class="btn btn-secondary btn-block">DASHBOARD</button>
        </form>  

      </p>
    </div>
  </div>
</div>

  
  
        

